<!DOCTYPE html>
<html>
<head>
    <title>User Data</title>
</head>
<body>
    <h1>User Data</h1>
    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Date of Birth</th>
                <th>Address</th>
                <th>Pincode</th>
                <th>Phone</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
                <tr>
                    <td>{{this.username}}</td>
                    <td>{{this.firstName}}</td>
                    <td>{{this.lastName}}</td>
                    <td>{{this.dob}}</td>
                    <td>{{this.address}}</td>
                    <td>{{this.pincode}}</td>
                    <td>{{this.phone}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</body>
</html>
